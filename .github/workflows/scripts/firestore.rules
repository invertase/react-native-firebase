rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  match /{document=**} {
      allow read, write: if false;
    }
  match /firestore/{document=**} {
      allow read, write: if true;
    } 
  match /{path=**}/collectionGroup/{documentId} {
      allow read, write: if true;
    }  
  }
}